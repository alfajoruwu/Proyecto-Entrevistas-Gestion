{
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "excel4node": "^1.8.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-sql-parser": "^5.3.10",
    "pg": "^8.16.0",
    "pdfkit": "^0.15.0",
    "sql-parser": "^0.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:usuarios": "jest --testPathPattern=usuarios",
    "test:auth": "jest --testPathPattern=auth",
    "test:tokens": "jest --testPathPattern=tokens"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "rutas/**/*.js",
      "middleware/**/*.js",
      "!**/node_modules/**"
    ]
  }
}